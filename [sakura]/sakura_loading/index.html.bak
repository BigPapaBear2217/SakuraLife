 	z-index: 10000;
	 position: relative;
	 width: 6px;
	 height: 24px;
	 background: #050505;
	 animation: pulse 750ms infinite;
	 animation-delay: 250ms;
}
 .loading-pulse:before, .loading-pulse:after {
 	z-index: 10000;
	 content: '';
	 position: absolute;
	 display: block;
	 height: 6px;
	 width: 6px;
	 background: #050505;
	 top: 50%;
	 transform: translateY(-50%);
	 animation: pulse 750ms infinite;
}
 .loading-pulse:before {
 	z-index: 10000;
	 left: -12px;
}
 .loading-pulse:after {
 	z-index: 10000;
	 left: 12px;
	 animation-delay: 500ms;
}
@keyframes spin {
  from {
    transform: rotate(0);
  }
  to{
    transform: rotate(359deg);
  }
}



@keyframes spin {
	 0% {
		 transform: rotate(0deg);
	}
	 100% {
		 transform: rotate(360deg);
	}
}
 @keyframes pulse {
	 50% {
		 background: #fff;
	}
